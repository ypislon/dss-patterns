     <title language="EN">No</title>
     <title language="FR">No</title>
     <title language="ES">No</title>
    </categoryMeasurementItem>
    <categoryMeasurementItem id="PHONOPHOBIA_YES" orderIndex="2">
     <title language="EN">Yes</title>
     <title language="FR">Oui</title>
     <title language="ES">Si</title>
    </categoryMeasurementItem>
   </categoryMeasurementItems>
  </categoryMeasurementType>
  <categoryMeasurementType id="HEADACHE_NAUSEA" entriesPerMeasurement="2">
   <title language="EN">Nausea</title>
   <title language="FR">Nausea</title>
   <title language="ES">Nausea</title>
   <categoryMeasurementItems>
    <categoryMeasurementItem id="NAUSEA_NO" orderIndex="1">
     <title language="EN">No</title>
     <title language="FR">No</title>
     <title language="ES">No</title>
    </categoryMeasurementItem>
    <categoryMeasurementItem id="NAUSEA_YES" orderIndex="2">
     <title language="EN">Yes</title>
